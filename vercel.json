{
    "version": 2,
    "builds": [
        {
            "src": "linstagramClient/package.json",
            "use": "@vercel/static-build"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/linstagramClient/index.html"
        }
    ],
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/linstagramClient/index.html"
        }
    ]
}